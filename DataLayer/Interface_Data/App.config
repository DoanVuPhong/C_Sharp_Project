                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8733/Design_Time_Addresses/Interface_Data/BusinessLogic/" />
                    </baseAddresses>
                </host>
            </service>
            <service name="Interface_Data.AuthorData">
